{
  "name": "flact-monorep",
  "version": "1.2.3",
  "description": "",
  "author": "Pixelplex",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:aws": "concurrently \"yarn build:api\" \"yarn build:db-check\" \"yarn build:mail\" \"yarn build:blockchain\" \"yarn build:scheduler\" \"yarn build:url-shortener\"",
    "build:all": "concurrently \"yarn build:api\" \"yarn build:db-check\" \"yarn build:mail\" \"yarn build:blockchain\" \"yarn build:console\" \"yarn build:scheduler\" \"yarn build:url-shortener\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build:console": "nest build console",
    "console": "node dist/apps/blockchain/console.js",
    "console:dev": "ts-node -r tsconfig-paths/register apps/blockchain/src/console.ts",
    "console:prod": "node dist/libs/console/main --",
    "build:api": "nest build api",
    "start:api": "nest start --watch api",
    "start:api:debug": "nest start --debug --watch api",
    "start:api:prod": "NODE_ENV=production node dist/apps/api/main api",
    "start:db-check": "nest start --watch db-check",
    "build:db-check": "nest build db-check",
    "start:db-check:prod": "NODE_ENV=production node dist/apps/db-check/main db-check",
    "build:blockchain": "nest build blockchain",
    "start:blockchain": "nest start --watch blockchain",
    "start:blockchain:debug": "nest start --debug --watch blockchain",
    "start:blockchain:prod": "node dist/apps/blockchain/main blockchain",
    "build:mail": "nest build mail",
    "start:mail": "nest start --watch mail",
    "start:mail:debug": "nest start --debug --watch mail",
    "start:mail:prod": "node dist/apps/mail/main mail",
    "build:scheduler": "nest build scheduler",
    "start:scheduler": "nest start --debug --watch scheduler",
    "start:scheduler:prod": "node dist/apps/scheduler/main scheduler",
    "build:url-shortener": "nest build url-shortener",
    "start:url-shortener": "nest start --debug --watch url-shortener",
    "start:url-shortener:prod": "node dist/apps/url-shortener/main url-shortener",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --collectCoverage --no-cache",
    "test:e2e": "jest --testRegex \".e2e-spec.ts$\" --runInBand",
    "test:e2e:api": "jest --testRegex \".e2e-spec.ts$\" --roots apps/api --runInBand",
    "test:e2e:url-shortener": "jest --testRegex \".e2e-spec.ts$\" --roots apps/url-shortener --runInBand",
    "typeorm": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "schema:sync": "yarn typeorm schema:sync",
    "migrate:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "migrate:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "audit": "audit-ci --config audit-ci.json"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.6",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.4.4",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/microservices": "^7.4.4",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/typeorm": "^7.1.4",
    "@pixelplex/sms-service": "^0.1.10",
    "@pixelplex/static-service": "^0.1.10",
    "@sentry/node": "^5.26.0",
    "@sentry/types": "^5.26.0",
    "@types/ramda": "^0.27.38",
    "amqp-connection-manager": "^3.2.1",
    "amqplib": "^0.6.0",
    "await-lock": "^2.1.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.12.2",
    "commander": "^6.2.0",
    "compression": "^1.7.4",
    "config": "^3.3.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron-validator": "^1.2.0",
    "express-status-monitor": "^1.3.3",
    "fabric-ca-client": "1.4.16",
    "fabric-network": "2.2.3",
    "fs-extra": "^9.0.1",
    "grpc": "^1.24.6",
    "jest-when": "^3.2.0",
    "libphonenumber-js": "^1.8.4",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "ms": "^2.1.3",
    "multer": "^1.4.2",
    "nano": "^9.0.3",
    "nestjs-console": "^4.0.0",
    "nodemailer": "^6.4.14",
    "nodemailer-express-handlebars": "^4.0.0",
    "object-mapper": "^6.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.1",
    "ramda": "^0.27.1",
    "randomstring": "^1.1.5",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.4",
    "testcontainers": "^6.0.0",
    "typeorm": "^0.2.32",
    "validator": "^13.1.17",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "audit-ci": "^3.1.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.0.3"
  },
  "resolutions": {
    "@pixelplex/sms-service/twilio/axios": "0.21.1",
    "express-status-monitor/axios": "0.21.1",
    "express-status-monitor/socket.io": "2.4.0",
    "tar": ">=4.4.18",
    "url-parse": ">=1.5.0",
    "xmlhttprequest-ssl": ">=1.6.1",
    "lodash": ">=4.17.21",
    "@grpc/grpc-js": ">=1.1.8"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "autoMapModuleNames": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "globalSetup": "<rootDir>/.test/setup.ts",
    "globalTeardown": "<rootDir>/.test/teardown.ts",
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "apps/api/src/**"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$",
      "main.ts$"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "flact/entities/(.*)": "<rootDir>/libs/entities/src/$1",
      "flact/entities": "<rootDir>/libs/entities/src",
      "flact/constants/(.*)": "<rootDir>/libs/constants/src/$1",
      "flact/constants": "<rootDir>/libs/constants/src",
      "flact/exceptions/(.*)": "<rootDir>/libs/exceptions/src/$1",
      "flact/exceptions": "<rootDir>/libs/exceptions/src",
      "flact/interfaces/(.*)": "<rootDir>/libs/interfaces/src/$1",
      "flact/interfaces": "<rootDir>/libs/interfaces/src",
      "flact/connectors/(.*)": "<rootDir>/libs/connectors/src/$1",
      "flact/connectors": "<rootDir>/libs/connectors/src",
      "flact/dtos/(.*)": "<rootDir>/libs/dtos/src/$1",
      "flact/dtos": "<rootDir>/libs/dtos/src",
      "flact/helpers/(.*)": "<rootDir>/libs/helpers/src/$1",
      "flact/helpers": "<rootDir>/libs/helpers/src",
      "flact/decorators/(.*)": "<rootDir>/libs/decorators/src/$1",
      "flact/decorators": "<rootDir>/libs/decorators/src",
      "flact/mappers": "<rootDir>/libs/mappers/src"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
